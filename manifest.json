{
  "manifest_version": 3,
  "name": "Stream Stack",
  "description": "Cross platform watchlist :D",
  "version": "1.0",
  "action": {
    "default_title": "Open Watchlist"
  },
  "side_panel" : {
    "default_path": "sidepanel.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": ["sidePanel", "tabs"]
}